# Production Environment with Mock Services
# Use this for deployment when external services are not available

# Enable mock modes for all services
VITE_BLOCKCHAIN_MOCK=true
VITE_QUANTUM_MOCK=true
VITE_BIOSENSOR_MOCK=true

# Mock service endpoints (not actually used in mock mode)
VITE_BLOCKCHAIN_RPC=http://localhost:8545
VITE_CHAIN_ID=1337
VITE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

VITE_QUANTUM_API=http://localhost:5000
VITE_BIOSENSOR_API=http://localhost:5001

# Feature flags
VITE_ENABLE_BLOCKCHAIN=true
VITE_ENABLE_QUANTUM=true
VITE_ENABLE_BIOSENSOR=true

# Performance
VITE_ENABLE_MONITORING=true
VITE_LOG_LEVEL=info

# Build
VITE_SOURCEMAP=false
